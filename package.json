{
  "name": "voice-welfare-agent",
  "version": "1.0.0",
  "description": "Voice-first, agentic AI system for welfare scheme identification and application in Marathi",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest",
    "dev": "nodemon src/index.js",
    "demo": "node src/demo.js",
